{
    "compilerOptions": {
      "jsx": "react",
      "module": "commonjs",
      "noImplicitAny": true,
      "outDir": "./build/",
      "preserveConstEnums": true,
      "removeComments": true,
      "sourceMap": true,
      "target": "es5"
    },
    "include": [
      "src/components/*.tsx",
      "src/lib/*.ts",
    ],
    "typedocOptions":{
      "mode": "modules",
      "out": "docs"
    }
  }